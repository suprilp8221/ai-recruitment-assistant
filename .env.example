# .env.example - Template for environment variables
# Copy this file to .env and fill in your actual values

# Database connection string
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/ai_recruit

# OpenAI API Key - Get yours from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY_HERE

# Secret key for JWT/session management (generate a random string)
SECRET_KEY=your-secret-key-here

# Frontend origin for CORS (where your frontend is hosted)
FRONTEND_ORIGIN=http://localhost:5500
